# =============================================================================
# Data Miner Environment Variables
# Copy to .env and fill in values
# =============================================================================

# Hugging Face token for private models (optional)
# Get from: https://huggingface.co/settings/tokens
HF_TOKEN=

# Database URL (defaults to local postgres)
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/data_miner

# Debug mode - disables heartbeat (for debugging only!)
# DATA_MINER_DEBUG=1
